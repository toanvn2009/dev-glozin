                                                    style="--aspect-ratio: {{ image.aspect_ratio }};"
                                                  {% endif %}
                                                >
                                                  <div
                                                    class="banner__item-media block rounded{% if last_block.settings.content_position != 'bellow' %} absolute inset-0{% endif %}"
                                                    style="--point:{{ image.presentation.focal_point }};--aspect-ratio: {{ image.aspect_ratio }};"
                                                  >
                                                    {% render 'responsive-image',
                                                      image: image,
                                                      type: 'other',
                                                      image_alt: banner_alt
                                                    %}
                                                  </div>
                                                  {% if heading != blank
                                                    or subheading != blank
                                                    or description != blank
                                                    or button_label != blank
                                                  %}
                                                    <div
                                                      class="banner__item-info bg-transparent color-{{ color_scheme_banner }} w-full{% if last_block.settings.content_position != 'bellow' %} p-sp-5{% else %} mt-10{% endif %} text-{{ last_block.settings.content_alignment }} relative z-1{% if last_block.settings.content_position == 'text_above_button' %} flex column space-between align-{{ last_block.settings.content_alignment }}{% endif %}"
                                                    >
                                                      {% if last_block.settings.content_position == 'text_above_button'
                                                      %}
                                                        <div>
                                                      {% endif %}
                                                      {% if subheading != blank %}
                                                        <div class="mb-sp-3 subheading_weight">
                                                          {{ subheading | escape }}
                                                        </div>
                                                      {% endif %}

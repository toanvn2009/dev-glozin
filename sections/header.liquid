                                              assign promotion_count = promotion_count | plus: 1
                                            endif
                                          endfor
                                        -%}
                                        <div
                                          class="flex gap-10 row-gap-10 mega-banner wrap flex-cols p-gutter-x"
                                          style="{% if last_block.settings.content_position != 'bellow' %}--col-number: 1;{% else %}--col-number: 2;{% endif %};--col-tablet: 2;--gap:1rem;"
                                        >
                                          {%- for i in (1..5) -%}
                                            {%- capture promotion -%}promotion_image_{{ i }}{%- endcapture -%}
                                            {%- capture promotion_subheading -%}promotion_subheading_{{ i }}{%- endcapture -%}
                                            {%- capture promotion_heading -%}promotion_heading_{{ i }}{%- endcapture -%}
                                            {%- capture promotion_description -%}promotion_description_{{ i }}{%- endcapture -%}
                                            {%- capture promotion_button_label -%}promotion_button_label_{{ i }}{%- endcapture -%}
                                            {%- capture promotion_button_link -%}promotion_button_link_{{ i }}{%- endcapture -%}
                                            {%- if last_block.settings[promotion] != blank -%}
                                              {%- liquid
                                                assign image = last_block.settings[promotion]
                                                assign subheading = last_block.settings[promotion_subheading]
                                                assign heading = last_block.settings[promotion_heading]
                                                assign description = last_block.settings[promotion_description]
                                                assign button_label = last_block.settings[promotion_button_label]
                                                assign banner_link = last_block.settings[promotion_button_link]

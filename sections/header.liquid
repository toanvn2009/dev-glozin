                                          >
                                            {%- liquid
                                              for product in last_block.settings.product_list
                                                render 'card-product', card_product: product, section_id: section.id, disable_show_video: true, container: section_width, column: product_column, column_mobile: 1, padding: 20, indexFor: forloop.index
                                              endfor
                                            -%}
                                          </div>
                                        </div>
                                      {%- when 'menu_collection' -%}
                                        <div class="py-sp-3 col-mega mega-collection px-gutter-x">
                                          {%- liquid
                                            assign heading = last_block.settings.heading
                                            assign collection_column = last_block.settings.collection_column
                                            assign count = 0
                                            for item in last_block.settings.collection
                                              assign count = count | plus: 1
                                            endfor
                                          -%}
                                          {%- if heading != blank -%}
                                            <h4 class="mb-sp-5 section-heading">
                                              {{ heading }}
                                            </h4>
                                          {%- endif -%}
                                          <div
                                            class="flex gap-10 row-gap-sp-5 wrap flex-cols"
                                            style="{% if count > 1 %}--col-number: 2{% else %}--col-number: 1;{% endif %};--col-tablet: 2;"
                                          >
                                            {% for collection in last_block.settings.collection %}
                                              {% render 'collection-item',
                                                card_collection: collection,
                                                section: section,
                                                container: section_width,
                                                column: collection_column,
                                                column_mobile: 1,
                                                padding: 20
                                              %}
                                            {% endfor %}
                                          </div>
                                        </div>
                                    {%- endcase -%}
                                  {%- endif -%}
                                </div>
                              </div>
                            </details>
                          {%- else -%}
                            <a
                              class="menu__item no-underline py-10 fs-big-2 heading-color{% if settings.menu_uppercase %} uppercase{% endif %} menu_weight"
                              href="{{ link.url }}"
                              aria-label="{{ link.title }}"
                            >
                              {{- link.title -}}
                            </a>
                          {%- endif -%}
                        {%- else -%}
                          {%- if link.links.size > 0 -%}
                            <details
                              is="details-mega-menu"
                              class="inline-flex w-full details-header-menu animate-motion"
                            >
                              <summary
                                class="menu__item level-1 transparent-menu heading-color{% if settings.menu_uppercase %} uppercase{% endif %} menu_weight flex w-full align-center gap-5 space-between"
                                data-href="{{ link.url }}"
                                aria-label="{{ link.title }}"
                              >
                                {{ link.title }}
                                {% if settings.rtl %}
                                  <svg class="block w-7 icon-down hidden-1025" width="7" height="13">
                                    <use href="#icon-back" />
                                  </svg>
                                {% else %}
                                  <svg class="block w-7 icon-down hidden-1025" width="7" height="13">
                                    <use href="#icon-next" />
                                  </svg>
                                {% endif %}

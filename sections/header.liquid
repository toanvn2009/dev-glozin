                              {%- render 'horizontal-menu',
                                section_st: section_st,
                                menu_mobile: true,
                                link: link,
                                type: 'classic',
                                level1_index: forloop.index
                              -%}
                            </details>
                          {%- else -%}
                            <a
                              class="menu__item level-1 transparent-menu heading-color{% if settings.menu_uppercase %} uppercase{% endif %} menu_weight"
                              href="{{ link.url }}"
                              aria-label="{{ link.title }}"
                            >
                              <span class="block outlined-border">{{- link.title -}}</span>
                            </a>
                          {%- endif -%}
                        {%- endif -%}
                      </li>
                    {%- else -%}
                      <li class="py-10">
                        {%- if link.links.size > 0 -%}
                          <details is="details-mega-menu" class="details-header-menu level-1">
                            <summary
                              class="menu__item outlined-border transparent-menu heading-color{% if settings.menu_uppercase %} uppercase{% endif %} menu_weight flex align-center gap-6 space-between"
                              data-href="{{ link.url }}"
                              aria-label="{{ link.title }}"
                            >
                              {{ link.title }}
                              {% if settings.rtl %}
                                <svg class="block w-7 icon-down hidden-1025" width="7" height="13">
                                  <use href="#icon-back" />
                                </svg>
                              {% else %}
                                <svg class="block w-7 icon-down hidden-1025" width="7" height="13">
                                  <use href="#icon-next" />
                                </svg>

  </div>
  <template class="menu-drawer">
    <menu-drawer id="Menu-Drawer" class="block modal active-modal-js" style="--modal-width: 100%;">
      <modal-overlay class="invisible modal-overlay transition-drawer cursor-crosshair z-21"></modal-overlay>
      <div
        class="modal-inner draw-mb{% if section_st.mobile_menu_position == "left" %} drawer-left drawer-left-mb left-0{% else %} drawer-right drawer-right-mb right-0{% endif %} inset-y-0 color-default transition-drawer w-full fixed z-22 bg-white overflow-hidden"

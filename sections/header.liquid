                                    link: link
                                  -%}
                                  {%- if has_content -%}
                                    {%- case last_block.type -%}
                                      {%- when 'menu_mega' -%}
                                        {%- liquid
                                          assign count = 0
                                          for item in last_block.settings.collection
                                            assign count = count | plus: 1
                                          endfor
                                          assign promotion_count = 0
                                          for i in (1..5)
                                            capture promotion
                                              echo 'promotion_image_' | append: i
                                            endcapture

{%- liquid
  assign section_st = section.settings
  assign color_scheme = section_st.color_scheme
  assign menu = section_st.menu
  assign transparent = section_st.transparent
  assign dropdowns_animation = section_st.dropdowns_animation
  if section_st.section_width == 'fix_width'
    assign section_width = 'container'
  elsif section_st.section_width == 'full_width'
    assign section_width = 'full-width'
  else
    assign section_width = 'fluid_container'
  endif
  assign transparent_class = ''
  if request.page_type == 'index' and transparent
    assign transparent_class = ' absolute inset-x-0 transparent z-15'
  endif
-%}
{%- capture style -%}
  --section-pt: {{ section_st.padding_top }};--section-pb: {{ section_st.padding_bottom }};--gap: 1rem; --gap-desktop: var(--sp-6);{{- -}}
  {% if transparent %}--transparent-color: {{ section_st.header_text_icon_transparent }};{% endif %}{{- -}}
{%- endcapture -%}

{% style %}
  @media only screen and (min-width: 1025px) {
    .logo-left{
      --header-grid-template: 'logo menu icon' / minmax(0, 1fr) fit-content(65%) minmax(0,1fr);
    }
    .logo-center{
      --header-grid-template: 'menu logo icon' / minmax(0, 1fr) minmax(50px, var(--logo-width))  minmax(0,1fr);
    }
  }
  {% if section_st.mobile_menu_position == "right" %}
    @media only screen and (max-width: 1024.98px) {
      .menu-mobile-right .mobile-menu-toggle  {
        grid-area: menu;
      }
      .menu-mobile-right {
        --header-grid-template: 'logo icon menu' / minmax(auto, 45%) minmax(0,1fr) auto;
      }
      .mobile-menu-toggle {
        margin-right: -1rem;
      }
    }
  {% endif %}
  {% if section_st.mobile_menu_position == "left" %}
    @media only screen and (max-width: 1024.98px) {
      .mobile-menu-toggle {
        margin-left: -1rem;
      }
    }
  {% endif %}
  {% if section_st.logo_position == 'left' %}
    .header-wrapper{
      --header-grid-template: 'menu logo icon' / 3.4rem minmax(auto, 45%)  minmax(10rem,1fr);{{- -}}
    }
    {% else %}
    .header-wrapper{
      --header-grid-template: 'menu logo icon' / minmax(4rem, 1fr) minmax(auto, 45%)  minmax(10rem,1fr);{{- -}}
    }
  {% endif %}
  {% if request.page_type == 'index' and transparent %}
    .site-header .transparent{
      background: transparent;
      transition: background 0.3s ease-in-out;
    }
    .site-header:not(.section-header-sticky) :is(.transparent-color,.heading-color.transparent-color){
      --color-text: {{ section_st.header_text_icon_transparent }};
      --color-heading: {{ section_st.header_text_icon_transparent }};
      color: var(--color-text);
    }
    @media (min-width: 1025px) {
      .site-header:not(.section-header-sticky) .transparent-menu{
        --color-text: {{ section_st.header_text_icon_transparent }};
        --color-heading: {{ section_st.header_text_icon_transparent }};
      }
      .header__action .button-localization{
        color: {{ section_st.header_text_icon_transparent }};
      }
    }
    @media (min-width: 992px) {
      body:has(.header-wrapper.transparent) .h-full_screen {
          min-height: 100vh;
      }
      body:has(.header-wrapper.transparent) .section-announcement-bar ~ .content-for-layout .h-full_screen {
        min-height: calc(100vh - var(--height-bar, 46px));
      }
    }
  {% endif %}
{% endstyle %}

{%- for block in section.blocks -%}
  {%- case block.type -%}
    {%- when 'menu_mega' -%}
      {% if request.visual_preview_mode %}
        <div {{ block.shopify_attributes }} class="flex w-full gap-10 bg-white p-30 rounded">
          <ul
            class="list-unstyled flex flex-1 gap-10 w-full"
          >
            {% for i in (1..3) %}
              <li
                class="menu-link have-child"
                style="width:25%;"
              >
                <a
                  class="menu__item level-2 heading-style h6 outlined-border relative"
                  href="#"
                  aria-label="{{ 'accessibility.heading_menu' | t }}"
                >
                  {{ 'accessibility.heading_menu' | t }}
                </a>

                <ul class="sub-children-menu list-unstyled flex column gap-sp-2 overflow-hidden-mb mt-1025-sp-4">
                  {% for i in (1..5) %}
                    <li class="menu-link">
                      <a
                        class="menu__item level-3 outlined-border relative"
                        href="#"
                        aria-label="{{ 'accessibility.menu_item' | t }}"
                      >
                        {{ 'accessibility.menu_item' | t }}
                      </a>
                    </li>
                  {% endfor %}
                </ul>
              </li>
            {% endfor %}

            <li
              class="menu-link have-child"
              style="width:25%;"
            >
              <div
                class="product-item__media--ratio rounded"
                style="--aspect-ratio: 2/3;"
              >
                {%- render 'placeholder-render', type: 'product' -%}
              </div>
            </li>
          </ul>
        </div>
      {% endif %}
    {%- when 'menu_product' -%}
      {% if request.visual_preview_mode %}
        <div {{ block.shopify_attributes }} class="flex w-full gap-10 bg-white p-30 rounded">
          <ul
            class="list-unstyled flex flex-1 gap-10 w-50-percent"
          >
            {% for i in (1..2) %}
              <li
                class="menu-link have-child w-50-percent"
              >
                <a
                  class="menu__item level-2 heading-style h6 outlined-border relative"
                  href="#"
                  aria-label="{{ 'accessibility.heading_menu' | t }}"
                >
                  {{ 'accessibility.heading_menu' | t }}
                </a>

                <ul class="sub-children-menu list-unstyled flex column gap-sp-2 overflow-hidden-mb mt-1025-sp-4">
                  {% for i in (1..5) %}
                    <li class="menu-link">
                      <a
                        class="menu__item level-3 outlined-border relative"
                        href="#"
                        aria-label="{{ 'accessibility.menu_item' | t }}"
                      >
                        {{ 'accessibility.menu_item' | t }}
                      </a>
                    </li>
                  {% endfor %}
                </ul>
              </li>
            {% endfor %}
          </ul>
          <div class="flex gap-10 w-50-percent">
            {% for i in (1..2) %}
              <div
                class="w-full "
              >
                {% render 'card-product' %}
              </div>
            {% endfor %}
          </div>
        </div>
      {% endif %}
    {%- when 'menu_collection' -%}
      {% if request.visual_preview_mode %}
        <div {{ block.shopify_attributes }} class="flex w-full gap-10 bg-white p-30 rounded">
          {% for i in (1..4) %}
            <div class="w-full">
              {% render 'collection-item', section: section, container: section.settings.section_width %}
            </div>
          {% endfor %}
        </div>
      {% endif %}
  {%- endcase -%}
{%- endfor -%}

<header
  is="site-header"
  data-sticky-type="{{ section_st.sticky_header_type }}"
  data-menu-trigger="{{ section_st.menu_trigger }}"
  data-dropdowns-animation="{{ dropdowns_animation }}"
  class="header-wrapper p-min relative z-10 transition-short {{ transparent_class }} color-{{ color_scheme }} gradient{% if request.page_type != 'index' %} border-bottom{% endif %}"
  style="{{ style | strip | strip_newlines }}"
>
  <div class="{{ section_width }}">
    <div
      class="header__inner grid gap logo-{{ section_st.logo_position }} menu-mobile-{{ section_st.mobile_menu_position }} align-center"
      style="{% if section_st.logo_position == 'left' %}--gap: 16px;{% else %}--gap: 12px;{% endif %}"
    >
      <toggle-menu class="relative flex content-center mobile-menu-toggle hidden-1025 fs-0 wh-44">
        <button
          class="btn-reset btn-toggle text-start pointer heading-color transparent-color flex header__icon"
          aria-label="{{ 'accessibility.toggle_menu' | t }}"
        >
          <svg width="44" height="44" viewBox="0 0 44 44" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M13 14.5H31" stroke="currentColor" stroke-width="1.8" stroke-linecap="round" stroke-linejoin="round"/>
            <path d="M13 19.5H22.47" stroke="currentColor" stroke-width="1.8" stroke-linecap="round" stroke-linejoin="round"/>
            <path d="M13 24.5H31" stroke="currentColor" stroke-width="1.8" stroke-linecap="round" stroke-linejoin="round"/>
            <path d="M13 29.5H22.47" stroke="currentColor" stroke-width="1.8" stroke-linecap="round" stroke-linejoin="round"/>
          </svg>
        </button>
      </toggle-menu>
      {%- render 'logo', section_st: section_st -%}
      <svg
        aria-hidden="true"
        focusable="false"
        role="presentation"
        class="hidden"
        hidden
      >
        <symbol id="icon-back" viewBox="0 0 6 11" fill="none">
          <path d="M4.7193 10.7605C4.85965 10.9202 5.03509 11 5.24561 11C5.45614 11 5.63158 10.9202 5.77193 10.7605C5.92398 10.6131 6 10.435 6 10.2261C6 10.005 5.92398 9.81463 5.77193 9.65494L1.80702 5.49079L5.77193 1.32663C5.92398 1.17923 6 1.00112 6 0.792295C6 0.571189 5.92398 0.380793 5.77193 0.221106C5.63158 0.0737018 5.45614 0 5.24561 0C5.03509 0 4.85965 0.0737018 4.7193 0.221106L0.22807 4.93802C0.0760236 5.09771 0 5.28811 0 5.50921C0 5.71803 0.0760236 5.89615 0.22807 6.04355L4.7193 10.7605Z" fill="currentColor"/>
        </symbol>
        <symbol id="icon-next" viewBox="0 0 6 11" fill="none">
          <path fill="currentColor" d="M1.278 10.721a.688.688 0 0 1-.528.229.688.688 0 0 1-.527-.229.669.669 0 0 1-.229-.51c0-.21.077-.392.229-.544l3.973-3.973L.223 1.721a.669.669 0 0 1-.229-.51c0-.21.077-.392.229-.544C.363.527.539.456.75.456c.211 0 .387.07.528.21l4.5 4.5a.744.744 0 0 1 .228.546c0 .199-.076.369-.228.51l-4.5 4.5Z"/>
        </symbol>
        <symbol id="icon-cart" viewBox="0 0 22 20" fill="none">
          <path d="M7.70016 5.25835C7.70016 7.45847 9.53922 8.3498 11.0002 8.3498C12.4611 8.3498 14.3002 7.45847 14.3002 5.25835M6.85649 0.924805H15.1362C19.046 0.924805 19.437 2.78663 19.7015 5.0583L20.7365 13.8405C21.0699 16.7211 20.196 19.0747 16.1711 19.0747H5.83303C1.79669 19.0747 0.922718 16.7211 1.2677 13.8405L2.30267 5.0583C2.55567 2.78663 2.94664 0.924805 6.85649 0.924805Z" stroke="currentColor" stroke-width="1.8" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/>

        </symbol>
        <symbol id="icon-arrow-down">
          <path d="M0.195652 1.58361C0.0652174 1.46321 0 1.31773 0 1.14716C0 0.966555 0.0652174 0.811037 0.195652 0.680602C0.316054 0.560201 0.466555 0.5 0.647157 0.5C0.827759 0.5 0.978261 0.560201 1.09866 0.680602L4.5 4.08194L7.90134 0.680602C8.02174 0.560201 8.17224 0.5 8.35284 0.5C8.53344 0.5 8.68395 0.560201 8.80435 0.680602C8.93478 0.811037 9 0.966555 9 1.14716C9 1.31773 8.93478 1.46321 8.80435 1.58361L4.9515 5.43646C4.8311 5.56689 4.6806 5.63211 4.5 5.63211C4.3194 5.63211 4.1689 5.56689 4.04849 5.43646L0.195652 1.58361Z" fill="currentColor"/>
        </symbol>
        <symbol id="icon-arrow-down-mb">
          <path d="M0.283083 1.49415C0.094362 1.33041 6.45254e-07 1.12573 6.34518e-07 0.880117C6.23781e-07 0.634503 0.0943619 0.429825 0.283083 0.266082C0.457288 0.0886945 0.667784 5.39058e-07 0.914574 5.28271e-07C1.17588 5.16849e-07 1.40089 0.0886945 1.58962 0.266082L6.51089 4.89181L11.4322 0.266082C11.6064 0.088694 11.8169 5.17166e-08 12.0637 4.09291e-08C12.325 2.9507e-08 12.55 0.088694 12.7387 0.266082C12.9129 0.429825 13 0.634503 13 0.880117C13 1.12573 12.9129 1.33041 12.7387 1.49415L7.16415 6.73392C6.97543 6.91131 6.75042 7 6.48911 7C6.24232 7 6.03183 6.91131 5.85762 6.73392L0.283083 1.49415Z" fill="currentColor"/>
        </symbol>
      </svg>
      {%- if menu != blank -%}
        <nav class="header__menu hidden block-1025 {{ section_st.menu_trigger }}">
          <ul class="flex list-menu list-unstyled wrap">
            {%- for link in menu.links -%}
              {%- if section.blocks.size > 0 -%}
                <li>
                  {%- liquid
                    assign last_block = null
                    assign mega_width = 100
                    for block in section.blocks
                      assign block_st = block.settings
                      if block_st.text == link.title
                        assign last_block = block
                      endif
                    endfor
                  -%}
                  {%- if last_block != null -%}
                    {%- assign has_content = false -%}
                    {%- case last_block.type -%}
                      {%- when 'menu_mega' -%}
                        {%- for i in (1..5) -%}
                          {%- capture promotion -%}promotion_image_{{ i }}{%- endcapture -%}
                          {%- liquid
                            if last_block.settings[promotion] != blank
                              assign has_content = true
                            endif
                          -%}
                        {%- endfor -%}
                      {%- when 'menu_product' -%}
                        {%- liquid
                          if last_block.settings.product_list != blank
                            assign has_content = true
                          endif
                        -%}
                      {%- when 'menu_collection' -%}
                        {%- liquid
                          if last_block.settings.collection != blank
                            assign has_content = true
                          endif
                        -%}
                    {%- endcase -%}
                    {%- if link.links.size > 0 or has_content -%}
                      <details
                        is="details-mega-menu"
                        class="inline-block details-header-menu level-1 animate-motion"
                        {{ last_block.shopify_attributes }}
                      >
                        <summary
                          class="menu__item outlined-border transparent-menu heading-color{% if settings.menu_uppercase %} uppercase{% endif %} menu_weight flex align-center gap-6 space-between"
                          data-href="{{ link.url }}"
                          aria-label="{{ link.title }}"
                        >
                          {{ link.title }}
                          <svg
                            class="icon icon-down icon-rotate transition-short"
                            width="9"
                            height="6"
                            viewBox="0 0 9 6"
                          >
                            <use href="#icon-arrow-down" />
                          </svg>
                        </summary>
                        {%- liquid
                          if has_content and link.links.size > 0
                            assign mega_width = last_block.settings.mega_width
                          endif
                        -%}
                        <div
                          class="mega-menu submenu z-5 submenu-desktop overflow-auto {{ dropdowns_animation }} border-top inset-x-0 absolute{% if dropdowns_animation != 'fade-in-down' %} z-2 bg-white{% endif %}"
                          style="--col-mega-width: {{ mega_width }}%; --gap: 20px; --delay: 0.05s;"
                        >
                          <div class="py-40 overflow-auto menu-container custom-scrollbar{% if dropdowns_animation == 'fade-in-down' %} bg-white{% endif %}">
                            <div class="container flex wrap gap">
                              {%- if link.links.size > 0 -%}
                                <div
                                  class="flex-1 menu-list"
                                  style="--col-desktop: {{ last_block.settings.menu_column }}"
                                >
                                  {%- render 'horizontal-menu', section_st: section_st, menu_mega: true, link: link -%}
                                </div>
                              {%- endif -%}
                              {%- if has_content -%}
                                {%- case last_block.type -%}
                                  {%- when 'menu_mega' -%}
                                    {% liquid
                                      assign promotion_count = 0
                                      for i in (1..5)
                                        capture promotion
                                          echo 'promotion_image_' | append: i
                                        endcapture
                                        if last_block.settings[promotion] != blank
                                          assign promotion_count = promotion_count | plus: 1
                                        endif
                                      endfor
                                    %}
                                    <div
                                      class="flex col-mega mega-banner wrap flex-cols"
                                      style="{% if last_block.settings.banner_column > 1 %}--col-number: 2{% else %}--col-number: 1;{% endif %};--col-desktop: {{ last_block.settings.banner_column }};"
                                    >
                                      {%- for i in (1..5) -%}
                                        {%- capture promotion -%}promotion_image_{{ i }}{%- endcapture -%}
                                        {%- capture promotion_subheading -%}promotion_subheading_{{ i }}{%- endcapture -%}
                                        {%- capture promotion_heading -%}promotion_heading_{{ i }}{%- endcapture -%}
                                        {%- capture promotion_description -%}promotion_description_{{ i }}{%- endcapture -%}
                                        {%- capture promotion_button_label -%}promotion_button_label_{{ i }}{%- endcapture -%}
                                        {%- capture promotion_button_link -%}promotion_button_link_{{ i }}{%- endcapture -%}
                                        {%- if last_block.settings[promotion] != blank -%}
                                          {% case i %}
                                            {% when 1 %}
                                              {% assign color_scheme_banner = last_block.settings.color_scheme_banner_1 %}
                                            {% when 2 %}
                                              {% assign color_scheme_banner = last_block.settings.color_scheme_banner_2 %}
                                            {% when 3 %}
                                              {% assign color_scheme_banner = last_block.settings.color_scheme_banner_3 %}
                                            {% when 4 %}
                                              {% assign color_scheme_banner = last_block.settings.color_scheme_banner_4 %}
                                            {% when 5 %}
                                              {% assign color_scheme_banner = last_block.settings.color_scheme_banner_5 %}
                                            {% else %}
                                              {% assign color_scheme_banner = 'default' %}
                                          {% endcase %}
                                          {%- liquid
                                            assign image = last_block.settings[promotion]
                                            assign subheading = last_block.settings[promotion_subheading]
                                            assign heading = last_block.settings[promotion_heading]
                                            assign description = last_block.settings[promotion_description]
                                            assign button_label = last_block.settings[promotion_button_label]
                                            assign banner_link = last_block.settings[promotion_button_link]
                                            assign banner_default = 'accessibility.promotion_banner' | t
                                            assign image_alt = image.alt | default: banner_default | escape
                                          -%}
                                          <div class="banner__item{% if last_block.settings.content_position != 'bellow' %} rounded{% endif %}">
                                            <a
                                              {% if banner_link != blank %}
                                                href="{{ banner_link }}"
                                              {% else %}
                                                role="link" aria-disabled="true"
                                              {% endif %}
                                              arial-label="{{ image_alt }}"
                                              class="banner__inner hover-effect focus-inset-inside{% if last_block.settings.content_position != 'bellow' %} flex relative{% if last_block.settings.content_position != 'text_above_button' %} align-{{ last_block.settings.content_position }}{% endif %}{% endif %}"
                                              {% if last_block.settings.content_position != 'bellow' %}
                                                style="--aspect-ratio: {{ image.aspect_ratio }};"
                                              {% endif %}
                                            >
                                              <div
                                                class="banner__item-media block rounded hover-effect{% if last_block.settings.content_position != 'bellow' %} absolute inset-0{% endif %}"
                                                style="--aspect-ratio: {{ image.aspect_ratio }};--point:{{ image.presentation.focal_point }};"
                                              >
                                                {% render 'responsive-image',
                                                  image: image,
                                                  type: 'other',
                                                  image_alt: image_alt
                                                %}
                                              </div>
                                              {% if heading != blank
                                                or subheading != blank
                                                or description != blank
                                                or button_label != blank
                                              %}
                                                <div class="banner__item-info bg-transparent color-{{ color_scheme_banner }} w-full{% if last_block.settings.content_position != 'bellow' %} p-30{% else %} mt-10{% endif %} text-{{ last_block.settings.content_alignment }} relative z-1{% if last_block.settings.content_position == 'text_above_button' %} flex column space-between align-{{ last_block.settings.content_alignment }}{% endif %}">
                                                  {% if last_block.settings.content_position == 'text_above_button' %}
                                                    <div>
                                                  {% endif %}
                                                  {% if subheading != blank %}
                                                    <div class="mb-sp-3 subheading_weight">
                                                      {{ subheading | escape }}
                                                    </div>
                                                  {% endif %}
                                                  {% if heading != blank %}
                                                    <h2 class="heading{% if last_block.settings.banner_column > 3 %} h5{% else %} h4{% endif %}">
                                                      {{ heading }}
                                                    </h2>
                                                  {% endif %}
                                                  {% if description != blank %}
                                                    <div class="mt-5 description">
                                                      {{ description }}
                                                    </div>
                                                  {% endif %}
                                                  {% if last_block.settings.content_position == 'text_above_button' %}
                                                    </div>
                                                  {% endif %}
                                                  {% if button_label != blank %}
                                                    <div class="inline-flex">
                                                      <button
                                                        class="mt-sp-5 text-center btn pointer btn-primary"
                                                        style="--btn-padding-x: 4.25rem;"
                                                        aria-label="{{ button_label }}"
                                                      >
                                                        <span class="relative btn-text z-1">{{ button_label }}</span>
                                                      </button>
                                                    </div>
                                                  {% endif %}
                                                </div>
                                              {%- endif -%}
                                            </a>
                                          </div>
                                        {%- endif -%}
                                      {%- endfor -%}
                                    </div>
                                  {%- when 'menu_product' -%}
                                    <div class="col-mega mega-product">
                                      {%- liquid
                                        assign heading = last_block.settings.heading
                                        assign product_column = last_block.settings.product_column
                                      -%}
                                      {%- if heading != blank -%}
                                        <h2 class="mb-sp-5 section-heading h4">
                                          {{ heading }}
                                        </h2>
                                      {%- endif -%}
                                      <slide-section
                                        class="swiper hover-action"
                                        data-section-id="{{ section.id }}"
                                        data-autoplay="false"
                                        data-effect="slide"
                                        data-loop="false"
                                        data-speed="300"
                                        data-spacing="20"
                                        data-mobile="2"
                                        data-desktop="{{ product_column }}"
                                        data-tablet="{{ product_column }}"
                                      >
                                        <div class="swiper-wrapper">
                                          {%- liquid
                                            for product in last_block.settings.product_list
                                              render 'card-product', card_product: product, section_id: section.id, class: ' swiper-slide', disable_show_video: true, container: section_width, column: product_column, column_mobile: 1, padding: 20, indexFor: forloop.index
                                            endfor
                                          -%}
                                        </div>
                                        <div class="swiper-pagination"></div>
                                      </slide-section>
                                    </div>
                                  {%- when 'menu_collection' -%}
                                    <div class="col-mega mega-collection">
                                      {%- liquid
                                        assign heading = last_block.settings.heading
                                        assign collection_column = last_block.settings.collection_column
                                      -%}
                                      {%- if heading != blank -%}
                                        <h2 class="mb-sp-5 section-heading h4">
                                          {{ heading }}
                                        </h2>
                                      {%- endif -%}
                                      <slide-section
                                        class="swiper hover-action"
                                        data-section-id="{{ section.id }}"
                                        data-autoplay="false"
                                        data-effect="slide"
                                        data-loop="false"
                                        data-speed="300"
                                        data-spacing="20"
                                        data-mobile="2"
                                        data-desktop="{{ collection_column }}"
                                        data-tablet="{{ collection_column }}"
                                      >
                                        <div class="swiper-wrapper">
                                          {% for collection in last_block.settings.collection %}
                                            <div class="collection-item swiper-slide">
                                              {% render 'collection-item',
                                                card_collection: collection,
                                                section: section,
                                                container: section_width,
                                                column: collection_column,
                                                column_mobile: 1,
                                                padding: 20
                                              %}
                                            </div>
                                          {% endfor %}
                                        </div>
                                        <div class="swiper-pagination"></div>
                                      </slide-section>
                                    </div>
                                {%- endcase -%}
                              {%- endif -%}
                            </div>
                          </div>
                        </div>
                      </details>
                    {%- else -%}
                      <a

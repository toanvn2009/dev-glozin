                        focusable='false'
                        role='presentation'
                        viewBox='0 0 24 24'
                        fill='none'
                        stroke='currentColor'
                        stroke-width='2'
                        stroke-linecap='round'
                        stroke-linejoin='round'
                        class='f:icon f:icon-arrow-right'
                      >

                  <x-media-gallery
                    id='Media-Gallery-{{ section.id }}'
                    class='x-product-media__gallery x-product-media__gallery--carousel'
                    data-layout='carousel'
                  >
                    <div class='x-product-media__list x-product-media--carousel'>
                      <x-carousel-component

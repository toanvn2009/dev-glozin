                                              else
                                                echo media | external_video_url: autoplay: true, loop: false | external_video_tag: class: video_class, loading: 'lazy'
                                              endif
                                            when 'video'
                                              echo media | media_tag: image_size: '2048x', autoplay: true, loop: false, controls: true, preload: 'none'
